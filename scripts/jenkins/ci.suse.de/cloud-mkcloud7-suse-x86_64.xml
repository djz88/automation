  <triggers class="vector">
    <hudson.triggers.TimerTrigger>
      <spec>H H * * *</spec>
    </hudson.triggers.TimerTrigger>
  </triggers>
  <builders>
    <com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
      <phaseName>Standard Gate Checks (non-HA/OVS+Linuxbridge)</phaseName>
      <continuationCondition>SUCCESSFUL</continuationCondition>
      <phaseJobs>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>openstack-mkcloud</jobName>
          <currParams>false</currParams>
          <configs>
            <hudson.plugins.parameterizedtrigger.PredefinedBuildParameters>
              <properties>cloudsource=susecloud7
TESTHEAD=
want_test_updates=1
nodenumber=2
mkcloudtarget=all_batch
scenario=cloud7-2nodes-default.yml
want_node_aliases=controller=1,compute-kvm=1
label=openstack-mkcloud-SLE12-x86_64
job_name=susecloud7: OVS
</properties>
            </hudson.plugins.parameterizedtrigger.PredefinedBuildParameters>
          </configs>
          <abortAllJob>false</abortAllJob>
        </com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>openstack-mkcloud</jobName>
          <currParams>false</currParams>
          <configs>
            <hudson.plugins.parameterizedtrigger.PredefinedBuildParameters>
              <properties>cloudsource=susecloud7
TESTHEAD=
want_test_updates=1
nodenumber=4
storage_method=ceph
networkingplugin=linuxbridge
mkcloudtarget=all
label=openstack-mkcloud-SLE12-x86_64
job_name=susecloud7: linuxbridge
</properties>
            </hudson.plugins.parameterizedtrigger.PredefinedBuildParameters>
          </configs>
          <abortAllJob>false</abortAllJob>
        </com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
      </phaseJobs>
    </com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
    <com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
      <phaseName>Extra Gate Checks (HA)</phaseName>
      <continuationCondition>SUCCESSFUL</continuationCondition>
      <phaseJobs>
        <com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
          <jobName>openstack-mkcloud</jobName>
          <currParams>false</currParams>
          <configs>
            <hudson.plugins.parameterizedtrigger.PredefinedBuildParameters>
              <properties>TESTHEAD=
want_test_updates=1
cloudsource=susecloud7
nodenumber=5
networkingmode=vxlan
hacloud=1
mkcloudtarget=all_noreboot
label=openstack-mkcloud-SLE12-x86_64
job_name=susecloud7: HA vxlan
</properties>
            </hudson.plugins.parameterizedtrigger.PredefinedBuildParameters>
          </configs>
          <abortAllJob>false</abortAllJob>
        </com.tikal.jenkins.plugins.multijob.PhaseJobsConfig>
      </phaseJobs>
    </com.tikal.jenkins.plugins.multijob.MultiJobBuilder>
